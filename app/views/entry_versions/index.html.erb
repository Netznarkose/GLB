<% content_for :content do %>
    <div class="funky_grey_seperator">
    <span class="right-action"> <%= link_to 'Aktuelle Version', entry_path(@entry), class: 'btn' %></span>
    <h4>Versions Index </h4>
	<% @entry_versions_all.to_enum.with_index(1) do |item, index| %>
		<p><h6><%= link_to "Version: #{index}) #{item.created_at.strftime('%b %d, %Y')}", entry_entry_version_path(item.item_id, index) %></h6></p>
	<% end %>
    </div>
<% end %>
