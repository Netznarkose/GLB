<% content_for :content do %>
	<% @entry_versions_all.each_with_index do |item, index| %>
		<% index_from_one = index + 1 %>
		<h4><%= link_to "Version: #{index_from_one}) #{item.created_at.strftime('%b %d, %Y')}", entry_entry_version_path(item.item_id, index_from_one) %></h4></br>
	<% end %>
<% end %>
